#include <iostream>

using namespace std;
 
 
bool isPrime(int num) {
    if (num <= 1) {
        return false; // Числа <= 1 не являются простыми
    }
    for (int i = 2; i * i <= num; ++i) { // Проверяем делители до квадратного корня
        if (num % i == 0) {
            return false; // Если нашли делитель, число не простое
        }
    }
    return true; // Если делителей не найдено, число простое
}


void printPrimes(int N) {
    cout << "Prime numbers not exceeding " << N << ": ";
    for (int i = 2; i <= N; ++i) {
        if (isPrime(i)) {
            cout << i << " "; // Выводим простое число
        }
    }
    cout << endl;
}

int main() {
    int N;

    cout << "Enter the number N: ";
    cin >> N;

    printPrimes(N); // Выводим простые числа, не превышающие N

    return 0;
}
